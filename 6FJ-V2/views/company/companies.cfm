<div class="page-companies">

	<article class="section companies well">
		<div class="container">

			<h1 class="page-title">Companies</h1>
			<h2 class="page-subtitle">Some of the companies and recruiters actively hiring.</h2>

			<section>
				<h2>Featured Companies</h2>

				<cfset request.cnt = 1 />
				
				<cfoutput>
				<cfloop index="request.c" from="1" to="#arrayLen(request.ary_companies)#">
					
					<cfif request.cnt EQ 1>
						<div class="row-fluid">
					</cfif>
						
						<div class="span4">
							<p>
								<a href="/company/#request.ary_companies[request.c].getSeoStrCompany()#">
									<cfif len(request.ary_companies[request.c].getStrMasterLogo())>
										<img src="#application.v1URL#/images/#request.ary_companies[request.c].getStrMasterLogo()#" alt="#request.ary_companies[request.c].getStrMasterLogoAltTag()#" class="image">
									<cfelse>
										<img src="http://placehold.it/272x155/f6f6f3/cccccc&text=Co" alt="Company image" class="image">
									</cfif>
								</a>
							</p>
							<h3><a href="/company/#request.ary_companies[request.c].getSeoStrCompany()#">#request.ary_companies[request.c].getStrCoName()#</a></h3>
                            
							<p>#left(request.ary_companies[request.c].getStrSummaryShort(), 150)#..</p>
                            
						</div>

					<cfif request.cnt EQ 3 OR request.c EQ arrayLen(request.ary_companies)>
						</div>
						<cfset request.cnt = 0 />
					</cfif>
					
					<cfset request.cnt ++>
					
				</cfloop>
				</cfoutput>
				
			</section>

			<section class="companies-by-industry">
				<h2>Companies Posting Jobs (by Industry)</h2>
				
				<div class="row-fluid">
					
					<cfset request.columnCnt = Ceiling(request.qry_industries.recordcount/3) />
				
					<cfset request.cntr = 0 />
					
					<cfloop query="request.qry_industries">
						
						<cfset request.cntr++ />
						
						<cfif request.cntr EQ 1>
							<ul class="span4">
						</cfif>
						
						<li><cfoutput><a href="/search?cat=#request.qry_industries.seoIndName#">#request.qry_industries.strIndName#</a></cfoutput></li>
							
						<cfif request.cntr EQ request.columnCnt>
							</ul>
							<cfset request.cntr = 0 />
						</cfif>
						
					</cfloop>
				
				</div>
			</section>

			<section class="all-companies">
				<h2>More Hiring Companies</h2>
				
				<cfset request.cnt = 1 />
				
				<cfoutput>
				<cfloop index="request.c" from="1" to="#arrayLen(request.ary_hiring)#">
					
					<cfif request.cnt EQ 1>
						<div class="row-fluid">
					</cfif>
						
						<div class="span2">
							<!---><a href="/company/#request.ary_hiring[request.c].getSeoStrCompany()#">--->
							<a href="/search?strSkills=#request.ary_hiring[request.c].getSeoStrCompany()#&strLocation=">
								<cfif len(request.ary_hiring[request.c].getStrMasterLogo())>
									<img src="#application.v1URL#/images/#request.ary_hiring[request.c].getStrMasterLogo()#" alt="#request.ary_hiring[request.c].getStrMasterLogoAltTag()#" class="image">
								<cfelse>
									<img src="http://placehold.it/272x155/f6f6f3/cccccc&text=Co" alt="Company image" class="image">
								</cfif>
							</a>
							<!---><h3><a href="/company/#request.ary_hiring[request.c].getSeoStrCompany()#">#request.ary_hiring[request.c].getStrCoName()#</a></h3>--->
							<h3><a href="/search?strSkills=#request.ary_hiring[request.c].getSeoStrCompany()#&strLocation=">#request.ary_hiring[request.c].getStrCoName()#</a></h3>
						</div>

					<cfif request.cnt EQ 6 OR request.c EQ arrayLen(request.ary_hiring)>
						</div>
						<cfset request.cnt = 0 />
					</cfif>
					
					<cfset request.cnt ++>
					
				</cfloop>
				</cfoutput>
				
			</section>

		</div>
	</article>

</div>
